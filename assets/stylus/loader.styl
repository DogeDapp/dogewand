iframe#dgw-frame
  position fixed
  bottom 0
  left 0
  z-index 100000

.dgw-dogewand
  font-family sans-serif
  .dgw-toolbar
    position fixed
    top 0
    right 0
    z-index 999999999
    background #4A4A4A

  .dgw-btn
    height 50px
    display inline-block



.dgw-wand
  *
    cursor url('/assets/images/wand.png'), auto

.dgw-link
  cursor url('/assets/images/wand_active.png'), auto !important
  display inline-block
  position relative

  padding 10px
  margin -10px

  padding-top 15px
  margin-top -15px
